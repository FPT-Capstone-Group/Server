{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "users_permissions_user"
    },
    "payment_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment-history.payment-history",
      "mappedBy": "users_permissions_user"
    },
    "PhoneNumber": {
      "type": "string"
    },
    "family": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::family.family",
      "inversedBy": "users"
    },
    "CCCDRegistrationDate": {
      "type": "string"
    },
    "CCCD": {
      "type": "string"
    },
    "FaceEncoding": {
      "type": "string"
    },
    "ownership": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ownership.ownership",
      "inversedBy": "users_permissions_users"
    },
    "user_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::citizen-history.citizen-history",
      "mappedBy": "users_permissions_user"
    }
  }
}
